{
  "presets": [ ["@babel/preset-env", { "modules": false }], "@babel/preset-typescript" ],
  "plugins": [
    ["module-resolver", {
      "root": ["./lib"],
      "alias": {
        "@source": "./lib"
      }
    }],
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}
